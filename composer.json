{
    "name": "qbus/qbbootstrap",
    "type": "typo3-cms-extension",
    "description": "Bootstrap content elements based on b13/container or gridelements",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://www.qbus.de",
    "support": {
        "issues": "https://github.com/qbus-agentur/qbbootstrap/issues"
    },
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "bfr@qbus.de",
            "role": "Developer",
            "homepage": "https://www.qbus.de"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "typo3/cms-core": "^12.4",
        "qbus/qbtools": "^4.0",
        "b13/container": "^2.2"
    },
    "suggests": {
        "b13/container": "^1.0",
        "gridelementsteam/gridelements": ">=3.2.0"
    },
    "autoload": {
        "psr-4": {
            "Qbus\\Qbbootstrap\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "qbbootstrap"
        }
    },
    "replace": {
        "typo3-ter/qbbootstrap": "self.version"
    }
}
